/* CSS Document */

* { padding:0; margin:0; font-family:Arial, Helvetica, sans-serif; font-size:13px;}
body { padding:0; margin:0; font-family:Arial, Helvetica, sans-serif; font-size:13px;}

/* menu */
.cont_menu { position:relative; float:left; width:155px;}
.cont_menu ul{ list-style:none outside none;}
.cont_menu ul li{float:left; margin-top:10px;}
.cont_menu ul li.nomargin{margin-top:0;}
.cont_menu ul li a.pad{ position:relative; float:left; width:155px; background-color:#fca000; color:#FFF; -moz-border-radius: 6px 6px 6px 6px; -webkit-border-radius:6px 6px 6px 6px; text-decoration:none;}
.cont_menu ul li a.pad p.link{ padding:6px 0px 6px 10px;}
.cont_menu ul li a.pad:hover{ background-image: url(../img/menu_bg1.gif); background-position:right; background-repeat:no-repeat;}
.cont_menu ul li a.pad.nofr{ background-position:right; background-repeat:no-repeat; }
.cont_menu ul li a.pad.nofr:hover{ background-image: url(#);}
.cont_menu ul li a.pad.nofr.vedi{ color:#999;}

.cont_menu ul li a.pad2{ position:relative; float:left; width:155px; background-color:#fca000; color:#FFF; -moz-border-radius: 6px 6px 0 0; -webkit-border-radius:6px 6px 0 0; text-decoration:none;}
.cont_menu ul li a.pad2.noradius{ position:relative; float:left; width:155px; background-color:#fca000; color:#FFF; -moz-border-radius: 0 0 0 0; -webkit-border-radius:0 0 0 0; text-decoration:none;}
.cont_menu ul li a.pad2.noradius_bianco{ position:relative; float:left; width:153px; background-color:#FFF; color:#000; -moz-border-radius: 0 0 0 0; -webkit-border-radius:0 0 0 0; text-decoration:none; border-right:1px solid #fca000; border-left:1px solid #fca000;}
.cont_menu ul li a.pad2.noradius_bianco.giu{ position:relative; float:left; width:153px; background-color:#FFF; color:#000; -moz-border-radius: 0 0 0 0; -webkit-border-radius:0 0 0 0; text-decoration:none; border-right:1px solid #fca000; border-bottom:1px solid #fca000; border-left:1px solid #fca000;}
.cont_menu ul li a.pad2.noradius_bianco:hover{ background-image: url(#); background-color:#eaf2fa;}
.cont_menu ul li a.pad2.noradius_bianco p{ padding:6px 0px 6px 9px;}
.cont_menu ul li a.pad2.noradius_bianco p.vedi{ padding:6px 0px 6px 9px; font-weight:bold; background:url(../img/menu_fr.gif) left center no-repeat;}
.cont_menu ul li a.pad2 p.link{ padding:6px 0px 6px 10px;}
.cont_menu ul li a.pad2:hover{ background-image: url(../img/menu_bg1.gif); background-position:right; background-repeat:no-repeat;}
.cont_menu ul li a.pad2.giu{-moz-border-radius: 0 0 6px 6px; -webkit-border-radius:0 0 6px 6px;}
.cont_menu ul li .pad2_riga{ position:relative; float:left; width:153px; height:1px; background-color:#fca000; text-align:center; border-right:1px solid #fca000; border-left:1px solid #fca000;}
/* end - menu */


