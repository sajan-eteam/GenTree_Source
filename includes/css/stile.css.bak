/*for button*/
.btn {
	border-radius:4px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	background:-moz-linear-gradient(center top , #FFFFFF, #EBEBEB) repeat scroll 0 0 #F3F3F3;
	/*background:url("../img/btn_bg.png") repeat-x scroll 0 2px #fff;*/
	border-color:#5b5b5b;
	border-style:solid;
	border-width:1px;
	color:#262626;
	cursor:pointer;
	font:11px Arial, Helvetica, sans-serif;
	margin:0;
	overflow:visible;
	padding:2px 4px;
	text-shadow:1px 1px 0 #FFFFFF;
	width: auto;
	min-width:70px;
}
.center {
	border-radius: 0;
	-moz-border-radius: 0;
	-webkit-border-radius: 0;

	background: #F6F6F6 -webkit-gradient(linear, 0% 0%, 0% 100%, from(white), to(#EFEFEF));
	height:22px;
}
.left {
	border-radius: 4px 0 0 4px;
	-webkit-border-top-right-radius: 0;
	-webkit-border-bottom-right-radius: 0;
	-moz-border-radius: 4px 0 0 4px;

	background: #F6F6F6 -webkit-gradient(linear, 0% 0%, 0% 100%, from(white), to(#EFEFEF));
	height:22px;
}
.right {
	border-radius: 0 4px 4px 0;
	-webkit-border-top-left-radius: 0;
	-webkit-border-bottom-left-radius: 0;
	-moz-border-radius: 0 4px 4px 0;

	background: #F6F6F6 -webkit-gradient(linear, 0% 0%, 0% 100%, from(white), to(#EFEFEF));
	height:22px;
}
.btn:hover {
	-moz-box-shadow: 0 0 3px #999999;
	/*background-position: 0 8px;*/
	border-color: #5b5b5b;
	color: #000;
	text-decoration: none;
}
.left_right {
	border-radius: 4px 4px 4px 4px;
	-webkit-border-top-left-radius: 4px;
	-webkit-border-top-right-radius: 4px;
	-webkit-border-bottom-left-radius: 4px;
	-webkit-border-bottom-right-radius: 4px;
	-moz-border-radius: 4px 4px 4px 4px;

	background: #F6F6F6 -webkit-gradient(linear, 0% 0%, 0% 100%, from(white), to(#EFEFEF));
	height:22px;

